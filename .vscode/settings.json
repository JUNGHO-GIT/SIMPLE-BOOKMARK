{
	"files.exclude": {
		"**/.git": false,
		"**/.svn": false,
		"**/.hg": false,
		"**/.DS_Store": false,
		"**/Thumbs.db": false,
		"**/CVS": false,
		"**/bower_components": false,
		"**/coverage": false,
		"**/tmp": false,
		"**/.mvn": false,
		"**/mvnw": false,
		"**/mvnw.cmd": false,
		"**/*history*": false,
		"**/*bookmark*": false,
		"**/node_modules": false,
		"**/dist": false,
		"**/out": false
