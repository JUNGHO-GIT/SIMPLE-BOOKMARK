{
  "$schema": "https://json.schemastore.org/tsconfig.json",
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
		"target": "ESNext",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"verbatimModuleSyntax": true,
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable",
			"WebWorker",
			"ScriptHost"
		]
	}
}